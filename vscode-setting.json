{
    "emmet.triggerExpansionOnTab": true,
    "workbench.startupEditor": "none",
    "editor.tabSize": 4,
    "editor.wrappingIndent": "none",
    "editor.fontSize": 18,
    "editor.lineHeight": 22,
    "editor.hover.enabled": true,
    "editor.smoothScrolling": true,
    "files.defaultLanguage": "html",
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "javascript"
    ],
    "editor.colorDecorators": false,
    "window.nativeTabs": true,
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "wordWrapColumn",
    "workbench.editor.showTabs": true,
    "workbench.sideBar.location": "left",
    "git.ignoreLegacyWarning": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.macOptionIsMeta": false,
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.fontFamily": "Meslo LG S for Powerline",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": false,
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": false,
    "gitlens.advanced.fileHistoryFollowsRenames": false,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#9C575C",
        "terminalCursor.foreground": "#EA5652",
        "sideBar.background": "#0A0A0A",
        "badge.background": "#FCCD43",
        "badge.foreground": "#000000",
        "activityBarBadge.background": "#FCCD43",
        "activityBarBadge.foreground": "#000000",
        "tab.activeBorderTop": "#FCCD43",
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "eslint.alwaysShowStatus": true,
    "tslint.configFile": "tslint.json",
    "tslint.jsEnable": true,
    "tslint.suppressWhileTypeErrorsPresent": true,
    "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "indentRainbow.includedLanguages": [
        "html",
        "sass",
        "scss",
        "css"
    ],
    "indentRainbow.colors": [
    "rgba(0,0,0,0.07)",
    "rgba(29,29,29,0.07)",
    "rgba(47,47,47,0.07)",
    "rgba(71,71,71,0.07)",
    "rgba(95,95,95,0.07)",
    "rgba(119,119,119,0.07)",
    "rgba(142,142,142,0.07)",
    "rgba(166,166,166,0.07)",
    "rgba(190,190,190,0.07)",
    "rgba(214,214,214,0.07)",
    "rgba(238,238,238,0.07)"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "window.zoomLevel": -2,
    "editor.detectIndentation": true,
    "editor.renderWhitespace": "selection",
    "workbench.iconTheme": "file-icons",
    "explorer.compactFolders": false,
    "editor.minimap.enabled": false,
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "switch:command"
    ],
    "editor.codeActionsOnSave": null,
    "editor.semanticTokenColorCustomizations": null,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.productIconTheme": "fluent-icons",
    "workbench.colorTheme": "Bear Theme",
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
}
